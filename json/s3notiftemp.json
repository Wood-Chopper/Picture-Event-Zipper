{
	"QueueConfiguration": {
		"Id": "notif",
		"Event": "s3:ObjectCreated:*",
		"Queue": "arn:aws:sqs:REGION:ACCOUNT_ID:SQS",
		"Filter": {
			"Key": {
				"FilterRules": [
					{
						"Name": "prefix",
						"Value": "resized/"
					}
				]
			}
		}
	},
	"CloudFunctionConfiguration": [
		{
			"LambdaFunctionArn": "arn:aws:lambda:REGION:ACCOUNT_ID:function:RESIZER_NAME",
			"Event": "s3:ObjectCreated:*",
			"Filter": {
				"Key": {
					"FilterRules": [
						{
							"Name": "prefix",
							"Value": "upload/"
						}
					]
				}
			}
		}
	]
}